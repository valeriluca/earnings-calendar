{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "www",
  "routes": [
    {
      "src": "/api/yahoo-earnings",
      "dest": "/api/yahoo-earnings.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

